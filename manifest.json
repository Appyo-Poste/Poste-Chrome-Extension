{
    "name": "Poste",
    "version": "0.0.8",
    "description": "A chrome extension for users to save a post to their Poste account",
    "manifest_version": 2,
    "permissions": ["storage"],
    "browser_action": {
        "default_popup": "dist/index.html",
        "default_title": "Poste"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
      
}